<clojure>(declare ^:dynamic *words*)</clojure>
<html>
<head>
<title><clojure>(println *title*)</clojure></title>
</head>
<body>

<p>
Method: <clojure>(println ($ method))</clojure><br />
Path: <clojure>(println ($ uri))</clojure><br />
Params: <clojure>(println ($ params))</clojure><br />
</p>


<clojure> 
  (doseq [x [1 2 3]] (println  "CHP Line " x "<br />"))
</clojure>

<clojure> 
(binding [*words* "some words"] (println *words* "<br />"))

(let [text-input (fn [name value]
                   (format (str "<label>%s</label><br />"
                                "<input type=\"text\""
                                "name=\"%s\""
                                "value=\"%s\" /><br />")
                           name
                           name
                           value))]

  (dotimes [_ 3]
    (println (text-input _ (str "value=" _)))))
</clojure>
</body>
